{
  "logger": {
    "level": "debug",
    "timestamp": false
  },
  "esclient": {
    "apiVersion": "5.6",
    "hosts": [
      {
        "host": "elasticsearch"
      }
    ]
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "1"
      }
    }
  },
  "api": {
    "textAnalyzer": "libpostal",
    "services": {
      "pip": {
        "url": "http://pip:4200"
      },
      "libpostal": {
        "url": "http://libpostal:4400"
      },
      "placeholder": {
        "url": "http://placeholder:4100"
      },
      "interpolation": {
        "url": "http://interpolation:4300"
      }
    },
    "defaultParameters": {
      "focus.point.lat": 46.61,
      "focus.point.lon": 2.45
    }
  },
  "imports": {
    "adminLookup": {
      "enabled": true
    },
    "geonames": {
      "datapath": "/data/geonames",
      "countryCode": "FR",
      "sourceURL": "http://example.com/geonames/"
    },
    "openstreetmap": {
      "download": [
        {
          "sourceURL": "https://download.geofabrik.de/europe/france/pays-de-la-loire-latest.osm.pbf"
        }
      ],
      "leveldbpath": "/tmp",
      "datapath": "/data/openstreetmap",
      "import": [
        {
          "filename": "pays-de-la-loire-latest.osm.pbf"
        }
      ]
    },
    "openaddresses": {
      "datapath": "/data/openaddresses",
      "files": [
        "fr/loire_atlantique.csv"
      ]
    },
    "polyline": {
      "datapath": "/data/polylines",
      "files": [
        "extract.0sv"
      ]
    },
    "whosonfirst": {
      "datapath": "/data/whosonfirst",
      "importVenues": false,
      "importPostalcodes": true,
      "importPlace": [
        "85683233"
      ]
    }
  }
}